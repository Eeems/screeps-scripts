{
    "compilerOptions": {
        "target": "es5",
        "types": ["node", "typed-screeps", "long", "webassembly-js-api", "assemblyscript-loader"],
        "sourceMap": false,
        "declaration": true,
        "newLine": "LF",
        "removeComments": false,
        "outDir": "dist"
    },
    "include": [
        "src/main.ts",
        "src/module/**/*.d.ts",
        "typings/**/*.d.ts"
    ],
    "exclude": [
        "./dist",
        "node_modules"
    ]
}
